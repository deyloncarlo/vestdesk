<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Produto.
    -->
    <changeSet id="20180722172715-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="configuracao_produto_id"
                                 baseTableName="produto"
                                 constraintName="fk_produto_configuracao_produto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="configuracao_produto"/>

        <addForeignKeyConstraint baseColumnNames="produtos_id"
                                 baseTableName="produto_lista_cor"
                                 constraintName="fk_produto_lista_cor_produtos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="produto"/>
        <addForeignKeyConstraint baseColumnNames="lista_cors_id"
                                 baseTableName="produto_lista_cor"
                                 constraintName="fk_produto_lista_cor_lista_cors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cor"/>
        
    </changeSet>
</databaseChangeLog>
